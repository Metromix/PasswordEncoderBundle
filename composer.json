{
  "name": "metromix/password-encoder-bundle",
  "description": "Metromix Password Encoder Bundle",
  "keywords": ["password"],
  "type": "symfony-bundle",
  "license": "MIT",
  "version": "1.0.3",
  "authors": [
    {
      "name": "Dominik Labudzinski",
      "email": "dominik@labudzinski.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0.0",
    "ext-libsodium": ">=1.0.6",
    "symfony/symfony": "~3.0",
    "symfony/security": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.0"
  },
  "autoload": {
    "psr-4": { "Metromix\\PasswordEncoderBundle\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Metromix\\PasswordEncoderBundle\\Tests\\": "src/" }
  },

  "target-dir": "Metromix/PasswordEncoderBundle"
}